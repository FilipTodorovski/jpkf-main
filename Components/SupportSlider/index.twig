<section class="jpkf-support-slider">
    <div class="container--small">
        <div class="pre-title">{{ pre_title }}</div>
        <div class="jpkf-support-slider__wrapper" is="flynt-support-slider">
            <div class="swiper-container">
                <div class="swiper-wrapper">
                    {% for slider in slider_list %}
                        <div class="swiper-slide">
                            {% if '.svg' in slider.image.src %}
                                <img
                                    src="{{ slider.image.src }}"
                                    alt="{{ slider.image.alt|e }}"
                                />
                            {% else %}
                                <img
                                    class="lazyload"
                                    src="{{ placeholderImage(slider.image.width / 2, slider.image.height / 2) }}"
                                    data-src="{{ slider.image.src|resizeDynamic(slider.image.width / 2) }}"
                                    data-srcset="{{ slider.image.src|resizeDynamic(slider.image.width / 2) }} 1x, {{ slider.image.src|resizeDynamic(slider.image.width) }} 2x"
                                    data-sizes="auto"
                                    alt="{{ slider.image.alt|e }}"
                                />
                            {% endif %}
                        </div>
                    {% endfor %}
                </div>
            </div>
            <div class="jpkf-support-slider__buttons">
                <div class="jpkf-support-slider__button jpkf-support-slider__button--prev">
